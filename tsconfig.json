{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "lib": [
      "ESNext",
      "dom"
    ],
    "types": [
      "unplugin-icons/types/solid",
      "vinxi/client",
      "vite/client",
    ],
    "isolatedModules": true,
    "baseUrl": "./",
    "paths": {
      "~/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "./src/**/*",
    ".eslintrc.cjs",
    "app.config.ts",
    "unocss.config.ts"
  ],
  "exclude": [
    "node_modules",
  ]
}
